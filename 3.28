#include <stdio.h>
#include <string.h>

#define MAXN 20
typedef enum {false==0, true==1 } bool;

bool palindrome(char* s);

int main()
{
    char s[MAXN];

    scanf("%s", s);
    if (palindrome(s) == true)
        printf("Yes\n");
    else
        printf("No\n");
    printf("%s\n", s);

    return 0;
}
